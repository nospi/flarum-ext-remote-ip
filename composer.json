{
    "name": "nospi/remoteip",
    "description": "A Flarum extension to get the user's real remote IP address when running behind an NGINX reverse proxy",
    "type": "flarum-extension",
    "require": {
        "flarum/core": "^1.8"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "nospi\\RemoteIp\\": "src/"
        }
    },
    "authors": [
        {
            "name": "nospi",
            "homepage": "https://github.com/nospi"
        }
    ],
    "extra": {
        "flarum-extension": {
            "title": "Remote IP",
            "icon": {
                "name": "fas fa-network-wired",
                "backgroundColor": "#0cce6b",
                "color": "#212121"
            }
        }
    }
}